{
    "name": "asseco-voice/laravel-comments",
    "description": "Laravel comment system",
    "keywords": [
        "php",
        "laravel",
        "comments",
        "microservices",
        "database"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Asseco\\Comments\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\Comments\\CommentServiceProvider"
            ]
        }
    }
}
